<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">



    <declare-styleable name="StarSkyView">

        <!--最远的星星循环一次移动的时间，单位ms近点星星的移动速度默认是远星星的一般-->
        <attr format="integer" name="one_cycle_time_ms" />

        <!--远处星星的数量，近点星星和远点星星的数量是一致的-->
        <attr format="integer" name="star_nums" />

        <!--流星头部的尺寸-->
        <attr format="integer|reference" name="meteor_head_size" />
    </declare-styleable>


    <attr format="dimension" name="progressTextSize"/>
    <attr format="color" name="progressTextColor"/>
    <attr format="dimension" name="progressTextOffset"/>
    <attr format="color" name="progressNormalColor"/>
    <attr format="color" name="progressReachColor"/>
    <attr format="dimension" name="progressNormalSize"/>
    <attr format="dimension" name="progressReachSize"/>
    <attr format="dimension" name="progressReachWidth"/>
    <attr format="dimension" name="progressNormalWidth"/>
    <attr format="boolean" name="progressTextVisible"/>
    <attr format="dimension" name="progressTextSkewX"/>
    <attr format="string" name="progressTextPrefix"/>
    <attr format="string" name="progressTextSuffix"/>
    <attr format="color" name="innerBackgroundColor"/>

    <declare-styleable name="HorizontalProgressView">
        <attr name="progressNormalColor"/>
        <attr name="progressReachColor"/>
        <attr name="progressTextColor"/>
        <attr name="progressTextSize"/>
        <attr name="progressTextOffset"/>
        <attr name="progressNormalSize"/>
        <attr name="progressReachSize"/>
        <attr format="enum" name="progressTextPosition">
            <enum name="TOP" value="1"/>
            <enum name="BOTTOM" value="-1"/>
            <enum name="CENTER" value="0"/>
        </attr>
        <attr name="progressTextVisible"/>
        <attr name="progressTextSkewX"/>
        <attr name="progressTextPrefix"/>
        <attr name="progressTextSuffix"/>
    </declare-styleable>

    <declare-styleable name="CircleProgressView">
        <attr name="progressNormalColor"/>
        <attr name="progressReachColor"/>
        <attr name="progressTextColor"/>
        <attr name="progressTextSize"/>
        <attr name="progressTextOffset"/>
        <attr name="progressNormalSize"/>
        <attr name="progressReachSize"/>
        <attr format="dimension" name="radius"/>
        <attr name="progressTextVisible"/>
        <attr format="integer" name="progressStartArc"/>
        <attr name="progressTextSkewX"/>
        <attr name="progressTextPrefix"/>
        <attr name="progressTextSuffix"/>
        <attr name="innerBackgroundColor"/>
        <attr format="enum" name="progressStyle">
            <enum name="Normal" value="0"/>
            <enum name="FillInner" value="1"/>
            <enum name="FillInnerArc" value="2"/>
        </attr>
        <attr format="color" name="innerProgressColor"/>
        <attr format="dimension" name="innerPadding"/>
        <attr format="color" name="outerColor"/>
        <attr format="dimension" name="outerSize"/>
        <attr format="boolean" name="reachCapRound"/>
    </declare-styleable>


    <declare-styleable name="MarqueeViewStyle">
        <attr name="mvInterval" format="integer|reference"/>
        <attr name="mvAnimDuration" format="integer|reference"/>
        <attr name="mvTextSize" format="dimension|reference"/>
        <attr name="mvTextColor" format="color|reference"/>
        <attr name="mvSingleLine" format="boolean"/>
        <attr name="mvGravity">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="mvDirection">
            <enum name="bottom_to_top" value="0"/>
            <enum name="top_to_bottom" value="1"/>
            <enum name="right_to_left" value="2"/>
            <enum name="left_to_right" value="3"/>
        </attr>
    </declare-styleable>



    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API level.
         ?android:attr/buttonBarStyle is new as of API 11 so this is
         necessary to support previous API levels. -->

    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="CurrentItemInfoView">
        <attr name="isFromFullScreen" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MineFragmentItemView">
        <attr name="mine_item_icon" format="reference" />
        <attr name="mine_item_des" format="string" />
        <attr name="mine_is_show_split_line" format="boolean" />
    </declare-styleable>

